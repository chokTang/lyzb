<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/fragment_review_share_order_img_relativeLayout"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginLeft="10dp"
    android:background="@android:color/white"
    >
    <ImageView
        android:id="@+id/fragment_review_share_order_img_ImageView"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:scaleType="centerCrop" />
    <ImageView
        android:id="@+id/fragment_review_share_order_img_delete_ImageView"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="40dp"
        android:src="@mipmap/btn_clear_image"/>

</RelativeLayout>