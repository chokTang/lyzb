<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorTen"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/order_status_linear_layout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/tabHeight"
        android:visibility="gone"
        android:background="@android:color/white"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView_my_points"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="2dp"
            android:layout_weight="1"
            android:background="@drawable/tab_selector"
            android:gravity="center"
            android:text="店铺积分"
            android:textColor="@drawable/tab_text_selector_two"
            android:textSize="@dimen/fontTwo" />

        <TextView
            android:id="@+id/textView_integration_details"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/tab_selector"
            android:gravity="center"
            android:text="积分明细"
            android:textColor="@drawable/tab_text_selector_two"
            android:textSize="@dimen/fontTwo" />

        <TextView
            android:id="@+id/textView_exchange"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/tab_selector"
            android:gravity="center"
            android:text="积分兑换"
            android:textColor="@drawable/tab_text_selector_two"
            android:textSize="@dimen/fontTwo"/>



    </LinearLayout>

    <FrameLayout
        android:id="@+id/framelayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </FrameLayout>

</LinearLayout>
